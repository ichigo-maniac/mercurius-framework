<?xml version="1.0" encoding="UTF-8"?>

<data-import xmlns="http://mercurius-framework.org/data-import">

    <!-- Products -->
    <insert entity-name="Product">
        <!-- Common column values -->
        <common-values>
            <column property="catalog" value-import-bean="uniqueCodeSearchBean">master_catalog</column>
            <column property="mainUnit" value-import-bean="catalogUniqueCodeSearchBean">master_catalog:pieces</column>
            <column property="units" value-import-bean="multiCatalogUniqueCodeSearchBean">master_catalog:pieces</column>
        </common-values>
        <!-- Entity values -->
        <values>
            <value>
                <column property="name">Test product</column>
                <column property="code">test_product</column>
            </value>
            <value>
                <column property="name">Test product 2</column>
                <column property="code">test_product_2</column>
            </value>
            <value>
                <column property="name">Test product 3</column>
                <column property="code">test_product_3</column>
            </value>
        </values>
    </insert>

</data-import>